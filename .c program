#include <stdio.h>
int main() {
  int n,tem,rem,result=0;
  printf("enter a number");
  scanf("%d",&n);
  temp=n;
  while(temp!=0){
    rem=temp%10;
    result=result+rem*rem*rem;
    temp=temp/10;
  }
  if(result==n){
    printf("it is prime number");
  }
  else{
    printf("it is not a prime number");
  }
  return 0;
